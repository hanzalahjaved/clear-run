          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- FOD 4: Wire -->
    <model name="fod_wire_1">
      <static>true</static>
      <pose>-30 7 0.012 0 0 0.3</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.002</radius>
              <length>0.15</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.1 0.1 0.1 1</ambient>
