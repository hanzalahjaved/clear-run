          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- FOD 5: Nut -->
    <model name="fod_nut_1">
      <static>true</static>
      <pose>5 -12 0.012 0 0 0.7</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.008</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.55 0.55 0.55 1</ambient>
            <diffuse>0.75 0.75 0.75 1</diffuse>
            <specular>0.9 0.9 0.9 1</specular>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.01</radius>
              <length>0.008</length>
            </cylinder>
          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- FOD 6: Rubber piece -->
    <model name="fod_rubber_1">
      <static>true</static>
      <pose>-20 10 0.015 0 0 2.1</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>0.08 0.04 0.015</size>
            </box>
          </geometry>
          <material>
            <ambient>0.1 0.1 0.1 1</ambient>
            <diffuse>0.15 0.15 0.15 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.08 0.04 0.015</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- FOD 7: Screw -->
    <model name="fod_screw_1">
      <static>true</static>
      <pose>35 2 0.012 0.2 0 0.9</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.004</radius>
              <length>0.025</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.5 0.5 0.5 1</ambient>
            <diffuse>0.7 0.7 0.7 1</diffuse>
            <specular>0.85 0.85 0.85 1</specular>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <cylinder>
              <radius>0.004</radius>
