            </cylinder>
          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- FOD 8: Plastic piece -->
    <model name="fod_plastic_1">
      <static>true</static>
      <pose>-5 -5 0.012 0 0 1.5</pose>
      <link name="link">
        <visual name="visual">
          <geometry>
            <box>
              <size>0.06 0.04 0.008</size>
            </box>
          </geometry>
          <material>
            <ambient>0.8 0.2 0.2 1</ambient>
